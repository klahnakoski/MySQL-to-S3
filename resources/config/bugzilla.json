{
	"fact_table":"bugs",
	"include": [
		"attachments"
	],
	"exclude": ["bugs_activity"],
	"reference_tables": ["profiles", "product", "tracking_flags", "groups"],
	"reference_only": [
		"profiles.login_name",
		"groups.name",
		"products.name",
		"components.name",
		"tracking_flags.name"
	],

	"database":{
		"host":"localhost",
		"port": 3306,
		"schema":"bugzilla",
		"$ref":"~/private.json#bugzilla",
		"debug":true
	}
}
