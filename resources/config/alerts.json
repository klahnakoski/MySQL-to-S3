{
	"extract": {
		"last": "output/alerts_last_run.json",
		"field": [
			"last_updated",
			"id"
		],
		"type": [
			"time",
			"number"
		],
		"start": [
			"1jan2015",
			0
		],
		"batch": [
			"week",
			1000
		]
	},
	"destination": "tests/output/test_alert_output.json",

//	"destination": {
//		"bucket": "active-data-treeherder-alert",
//		"public": true,
//		"key_format": "a",
//		"$ref": "file://~/private.json#aws_credentials"
//	},
	"snowflake": {
		"fact_table": "performance_alert_summary",
		"show_foreign_keys": false,
		"null_values": [
			"-",
			"unknown",
			""
		],
		"add_relations": [
			"treeherder.performance_datum.ds_job_id -> treeherder.job.id",
			"treeherder.performance_datum.job_id -> treeherder.job.id",
			"treeherder.performance_datum.push_id -> treeherder.push.id",
			"treeherder.performance_datum.repository_id -> treeherder.repository.id",
			"treeherder.performance_datum.signature_id -> treeherder.performance_signature.id"
		],
		"name_relations": {
			"performance_alert_summary.id <- performance_alert.summary_id": "details",
			"performance_alert_summary.id <- performance_alert.related_summary_id": "see_also",
		},
		"include": [
		],
		"exclude": [
			"job",
			"runnable_job",
			"auth_user",
			"job_log",
			"text_log_step",
			"performance_datum.signature_id"
		],
		"exclude_path":[
			"series_signature.parent_signature",
			"series_signature.performance_datum",
			"performance_signature.performance_alert_summary"
		],
		"reference_only": [
			"option_collection.option",
			"push.revision",
			"performance_signature",
			"performance_framework.name",
			"user.email",
			"repository.name",
			"machine_platform.platform",
			"failure_classification.name",
			"option.name"
		],
		"database": {
			"schema": "treeherder",
			"username": "activedata",
			"$ref": "~/private.json#treeherder"
		}
	},
	"constants":{
		"mysql_to_s3.snowflake_schema.DEBUG": true,
		"pyLibrary.sql.mysql.DEBUG": true,
	},
	"debug": {
		"trace": true,
		"cprofile": false
	}
}
