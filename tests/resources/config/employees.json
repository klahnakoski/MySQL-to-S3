{
	"extract": {
		"last": "tests/output/employees_last_run.json",
		"field": "id",
		"type": "number",
		"start": 0,
		"batch": 100,
		"ids": "select id from employees where last_modified>='{{last.id}}' order by id limit 101"
	},
	"destination": {
		"bucket": "active-data-treeherder-jobs",
		"public": true,
		"key_format": "a",
		"$ref": "file://~/private.json#aws_credentials"
	},
	"snowflake": {
		"fact_table": "employees",
		"show_foreign_keys": true,
		"null_values": [
		],
		"add_relations": [
		],
		"include": [
		],
		"exclude": [
		],
		"reference_only": [
		],
		"database": {
			"schema": "localhost",
			"username": "root",
			"$ref": "~/private.json#test_database"
		}
	},
	"debug":{
		"trace":true
	}
}
